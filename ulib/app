#include <iostream>

namespace factorial
{
  static const void execute(int n = 0)
  {
    int factorial = 1;
    if (n <= 1)
    {
      std::cout << factorial << std::endl;
      return;
    }
    for (signed short i = n; i > 1; --i)
    {
      factorial = factorial * i;
    }
    std::cout << factorial << std::endl
              << std::flush;
    return;
  }
}